{
  "name": "FastFlow",
  "productName": "FastFlow",
  "companyName": "FastFlow",
  "version": "0.2.5",
  "description": "Debate application for keeping your files organized",
  "main": "main.js",
  "scripts": {
    "test": "echo lol no",
    "lint": "eslint app test *.js",
    "hot-server": "node server.js",
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "start-hot": "better-npm-run start-hot",
    "package": "better-npm-run package",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json"
  },
  "betterScripts": {
    "start": {
      "command": "electron ./",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-hot": {
      "command": "electron ./",
      "env": {
        "HOT": 1,
        "NODE_ENV": "development"
      }
    },
    "package": {
      "command": "node package.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.production.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FastFlowDebate/fastflow.git"
  },
  "author": "Adam Towers <adam.towers@icloud.com> (https://github.com/zarkoix)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FastFlowDebate/FastFlow/issues"
  },
  "keywords": [
    "electron",
    "debate"
  ],
  "homepage": "https://github.com/FastFlowDebate/FastFlow#readme",
  "devDependencies": {
    "better-npm-run": "0.0.5",
    "chromedriver": "^2.19.0",
    "del": "^2.2.0",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.36.0",
    "electron-rebuild": "^1.0.0",
    "eslint": "^1.3.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "fbjs-scripts": "^0.5.0",
    "jsdom": "^7.0.2",
    "node-gyp": "^3.2.1",
    "webpack": "^1.12.1",
    "webpack-target-electron-renderer": "^0.3.0"
  },
  "dependencies": {
    "alloyeditor": "^1.2.2",
    "angular": "^1.5.0",
    "angular-animate": "^1.5.6",
    "angular-mass-autocomplete": "^0.2.3",
    "angular-sanitize": "^1.5.6",
    "electron-debug": "^0.5.1",
    "font-awesome": "^4.4.0",
    "fuse.js": "^2.2.0",
    "jquery": "^2.1.4",
    "lokijs": "^1.3.11",
    "path": "^0.12.7",
    "regex": "^0.1.1"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
